<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_match_form" model="ir.ui.view">
        <field name="name">NAME OF Match LIST VIEW</field>
        <field name="model">estate.property.match</field>
        <field name="arch" type="xml">
            <form string="Matched Properties">
                <group>
                    <field name="to_match" string="Property id to Match"/>
                </group>
                <footer>
                     <!-- here the name="action_button_next2" matched MyTransientModel.action_button_next2() function" -->
                   <button name="action_button_next2" string="Next2" type="object" class="btn-primary"/>
                   <button name="cancel" string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>